{
  "source": {
    "exclude": [
      "node_modules/",
      "**/node_modules/"
    ],
    "excludePattern": "(^|\\/|\\\\)_|node_modules"
  },
  "tags": {
    "allowUnknownTags": ["category", "subcategory"]
  },
  "opts": {
    "template": "node_modules/better-docs"
  },
  "plugins": [
    "node_modules/better-docs/category"
  ]
}