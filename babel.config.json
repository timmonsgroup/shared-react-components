{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                },
                "useBuiltIns": "usage",
                "corejs": "3.6.5",
                "modules": false
            }
        ],
        [
            "@babel/preset-react",
            {
                "runtime": "automatic",
                "useBuiltIns": true
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-export-default-from",
        "babel-plugin-add-import-extension",
        "babel-plugin-proptypes-require"
    ]
}