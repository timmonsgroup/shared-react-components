{
  "name": "@timmons-group/shared-react-components",
  "version": "0.0.0",
  "private": true,
  "source": "src/lib.package.js",
  "main": "dist/shared-react-components.js",
  "type": "module",
  "umd:main": "dist/shared-react-components.umd.js",
  "module": "dist/shared-react-components.mjs",
  "exports": {
    "require": "./dist/shared-react-components.js",
    "default": "./dist/shared-react-components.modern.mjs"
  },
  "types": "dist/shared-react-components.d.ts",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lib": "microbundle --jsx React.createElement --jsxImportSource react --globals react/jsx-runtime=jsx --format modern,cjs",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4"
  },
  "theseSHOULDbeInPeerButCantCauseReasons":{
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6"
  },
  "devDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "microbundle": "^0.15.1",
    "prop-types": "^15.8.1",
    "webpack": "^5.74.0"
  }
}
