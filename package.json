{
  "name": "@timmons-group/shared-react-components",
  "version": "0.3.10",
  "private": false,
  "eslintIgnore": [
    "node_modules",
    "build/*"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./hooks": "./dist/hooks/index.js",
    "./helpers": "./dist/helpers/index.js",
    "./constants": "./dist/constants.js"
  },
  "scripts": {
    "clean": "npm run clean:root && npm run clean:hooks && npm run clean:hooks && npm run clean:stories",
    "clean:hooks": "rimraf dist/hooks/*",
    "clean:helpers": "rimraf dist/helpers/*",
    "clean:stories": "rimraf dist/stories/*",
    "clean:root": "rimraf dist/*.js",
    "build:package": "npm run clean && npm run build:components && npm run build:hooks && npm run build:helpers && npm run build:index",
    "build:package-localdev": "npm run build:components && npm run build:hooks && npm run build:helpers && npm run build:index",
    "build-storybook": "build-storybook -s public",
    "build:components": "set NODE_ENV=production&&babel src/src/stories --out-dir dist/stories --copy-files",
    "build:hooks": "set NODE_ENV=production&&babel src/src/hooks --out-dir dist/hooks --copy-files",
    "build:helpers": "set NODE_ENV=production&&babel src/src/helpers --out-dir dist/helpers --copy-files",
    "build:index": "set NODE_ENV=production&&babel src/src/index.js src/src/constants.js --out-dir dist --copy-files",
    "postinstall": "cd src && npm i"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@mui/x-data-grid": "^5.17.6",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-proptypes-require": "^1.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prop-types": "^15.8.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.74.0"
  }
}